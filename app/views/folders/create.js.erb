<% unless @remote_error %>
    $('.modal.new.folder').modal('hide');
    $('.row.list.folders').empty().append("<%= j render 'folders_list', folders: @folders, folder_shortcuts: @folder_shortcuts %>");    
    dummyBindingPopup();
    setToast('success', 'Đã tạo folder.');
<% else %>    
    $('.modal.new.folder').modal('hide');    
    setToast('warning', 'Cần có tên folder.');
<% end %>