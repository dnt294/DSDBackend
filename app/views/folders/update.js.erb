<% unless @remote_error %>
    $('.modal.rename').modal('hide');
    $('.row.list.folders').empty().append("<%= j render 'folders_list', folders: @folders, folder_shortcuts: @folder_shortcuts %>");
    dummyBindingPopup();
    setToast('success', 'Đã đổi tên folder.');
<% else %>    
    $('.modal.rename').modal('hide');    
    setToast('warning', 'Không đổi được tên.');
<% end %>